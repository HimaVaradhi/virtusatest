<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dimen_10"
    android:layout_marginRight="@dimen/dimen_10"
    android:layout_marginTop="@dimen/dimen_8"
    app:cardCornerRadius="@dimen/dimen_8"
    app:cardElevation="@dimen/dimen_5"
    app:cardUseCompatPadding="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/dimen_8"
        android:padding="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_margin="@dimen/dimen_8"
            android:padding="15dp">

            <TextView
                android:id="@+id/tv_Create_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:hint="2022-01-24T17:02:23.729Z"
                android:layout_marginEnd="@dimen/dimen_10"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/tv_is_occupied"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/tv_Create_date"
                tools:ignore="MissingConstraints"
                style="@style/MyActionBarTheme"
                android:layout_marginTop="2dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:hint="true"
                android:layout_marginEnd="@dimen/dimen_10"
                android:textSize="14sp"/>


            <TextView
                android:id="@+id/tv_max_occupancy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/tv_is_occupied"
                tools:ignore="MissingConstraints"
                style="@style/MyActionBarTheme"
                android:layout_marginTop="2dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:textColor="@color/black"
                android:hint="67"
                android:layout_marginEnd="@dimen/dimen_10"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/tv_id2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/tv_max_occupancy"
                tools:ignore="MissingConstraints"
                style="@style/MyActionBarTheme"
                android:layout_marginTop="2dp"
                android:textStyle="bold"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:textColor="@color/black"
                android:hint="1"
                android:textSize="14sp"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>